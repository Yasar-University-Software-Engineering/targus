<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.targus.ui.EmptyController">
    <VBox>
        <HBox>
            <Label fx:id="gaProgressLabel" text="GA progress:"/>

            <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0"/>
        </HBox>

        <VBox prefHeight="200.0" prefWidth="174.0" spacing="10.0">
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="86.0">
                <Circle fill="RED" radius="10.0" stroke="RED" strokeType="INSIDE"/>
                <Label text=": Target">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                <Circle fill="WHITE" radius="10.0" stroke="RED" strokeType="INSIDE"/>
                <Label text=": Potential Position">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                <Circle fill="GREEN" radius="10.0" stroke="BLACK" strokeType="INSIDE"/>
                <Label prefHeight="17.0" prefWidth="51.0" text=": Sensor">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                <Circle fill="WHITE" radius="10.0" stroke="BLUE" strokeType="INSIDE"/>
                <Label prefHeight="17.0" prefWidth="150.0" text=": Sensing Range">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="200.0">
                <Circle fill="WHITE" radius="10.0" stroke="ORANGE" strokeType="INSIDE"/>
                <Label prefHeight="17.0" prefWidth="150.0" text=": Communication Range">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
            </HBox>
        </VBox>
    </VBox>

</AnchorPane>
