<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0"
            fx:controller="com.targus.ui.controllers.InputsController">
    <VBox alignment="CENTER" prefHeight="206.0" prefWidth="218.0" spacing="10.0">
        <HBox alignment="CENTER_LEFT" prefHeight="18.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="124.0" text="Connectivity (m)" />
            <TextField fx:id="txtM" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="124.0" text="Coverage (k)" />
            <TextField fx:id="txtK" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="18.0" prefWidth="124.0" text="Communication Range" />
            <TextField fx:id="txtCommunicationRange" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="124.0" text="Sensing Range" />
            <TextField fx:id="txtSensingRange" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="124.0" text="Generation Count" />
            <TextField fx:id="txtGenerationCount" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="28.0" prefWidth="200.0" spacing="20.0">
            <Label alignment="CENTER_RIGHT" prefHeight="17.0" prefWidth="124.0" text="Mutation Rate" />
            <TextField fx:id="txtMutationRate" alignment="CENTER" prefHeight="25.0" prefWidth="71.0" />
        </HBox>
    </VBox>
</AnchorPane>
